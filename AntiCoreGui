local CoreGui = game:GetService("CoreGui")
local Images = {}

for i,v in next,CoreGui:GetDescendants() do
     if v:IsA("ImageLabel") and v.Image:find("rbxasset://") then
        table.insert(Images,v.Image)
    end
end

local Old

Old = hookmetamethod(game,"__namecall",newcclosure(function(Event,...)
    local Args = {...}
    
    if type(Args[1]) == "table" and table.find(Args[1],CoreGui) and Event == game:GetService("ContentProvider") and getnamecallmethod:lower() == "preloadasync" then
        Args[1] = Images
        
        return Old(Event,unpack(Args))
    end
    
    return Old(Event,...)
end))
